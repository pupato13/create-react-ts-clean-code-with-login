{
    "name": "create-react-ts-clean-code-with-login",
    "version": "0.1.0",
    "description": "A React clean code starter from scratch using TypeScript, Webpack, Jest and Axios",
    "bin": "./bin/generate-app.js",
    "author": "Diego Pupato",
    "main": "index.js",
    "scripts": {
        "dev:base": "webpack-dev-server --config webpack.dev.js",
        "dev": "npm run dev:base -- --open",
        "start": "webpack-cli serve --open --mode development --config webpack.dev.js",
        "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --passWithNoTests --no-cache --runInBand",
        "test:watch": "npm test -- --watch",
        "test:staged": "npm test -- --findRelatedTests",
        "test:ci": "npm test -- --coverage"
    },
    "repository": {
        "type": "git",
        "directory": "https://github.com/pupato13/create-react-ts-clean-code-with-login"
    },
    "keywords": [
        "react",
        "typescript",
        "clean code",
        "webpack",
        "jest"
    ],
    "license": "MIT",
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/axios": "^0.14.0",
        "@types/faker": "^5.5.8",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.16.3",
        "@types/react": "^18.2.2",
        "@types/react-dom": "^18.2.3",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.3",
        "eslint": "^8.39.0",
        "eslint-config-standard-with-typescript": "^34.0.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-standard": "^5.0.0",
        "faker": "^5.5.3",
        "git-commit-msg-linter": "^4.11.0",
        "html-webpack-plugin": "^5.5.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "node-sass": "^8.0.0",
        "sass-loader": "^13.2.2",
        "style-loader": "^3.3.2",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.2",
        "typescript": "^5.0.4",
        "webpack": "^5.82.0",
        "webpack-cli": "^5.0.2",
        "webpack-dev-server": "^4.13.3"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.11.1",
        "rimraf": "^5.0.0"
    }
}
